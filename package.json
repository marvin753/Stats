{
  "name": "quiz-scraper",
  "version": "1.0.0",
  "description": "DOM scraper for extracting quiz questions and answers",
  "main": "scraper.js",
  "scripts": {
    "scrape": "node scraper.js",
    "scrape:url": "node scraper.js --url",
    "ai-parser": "node ai-parser-service.js",
    "test": "jest",
    "test:backend": "jest backend/tests",
    "test:frontend": "jest frontend/tests",
    "test:e2e": "jest tests/e2e.test.js",
    "test:security": "jest backend/tests/security.test.js",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathIgnorePatterns=integration.test.js --testPathIgnorePatterns=e2e.test.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:verbose": "jest --verbose"
  },
  "keywords": [
    "scraper",
    "playwright",
    "quiz",
    "dom"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "playwright": "^1.40.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@types/jest": "^30.0.0",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-html-reporter": "^3.10.2",
    "jest-junit": "^16.0.0",
    "jest-watch-typeahead": "^2.2.2",
    "nock": "^13.5.6",
    "supertest": "^6.3.4"
  }
}
